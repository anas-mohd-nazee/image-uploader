    "@types/jasmine": "^3.5.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.4",
    "@types/sharp": "^0.26.0",
    "jasmine": "^3.6.2",
    "jasmine-auto-spies": "^6.3.4",
    "jasmine-spec-reporter": "^6.0.0",
    "ts-node": "^9.0.0",
    "tsc": "^1.20150623.0",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  }
