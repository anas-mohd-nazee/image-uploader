{
  "name": "image-upload-service",
  "version": "1.0.0",
  "description": "Image Upload Service",
  "main": "dist/app/server.js",
  "scripts": {
    "prebuild": "rmdir /S /Q .\\dist\\ && tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc && copy package.json dist\\package.json",
    "prestart": "npm run build",
    "start": "ts-node .",
    "watch": "tsc-watch --project . --outDir ./dist --onSuccess \"ts-node . --log-level=verbose --log-flag=date\"",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json"
  },
  "author": "anas.mohdnazlee@gmail.com",
  "dependencies": {
    "@barusu/chalk-logger": "0.0.35",
    "adm-zip": "^0.4.16",
    "argon2": "^0.27.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "image-type": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "read-chunk": "^3.2.0",
    "rxjs": "^6.6.3",
    "sharp": "^0.26.2",
    "typescript-ioc": "^3.2.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/body-parser": "^1.19.0",
